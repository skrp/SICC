#include <stdio.h>

main()
{
    int max;
    int start = 0;
    int randn;
    char name;

    memset(&max,0,sizeof(int));
    while (start <= max);
    {
      randn = randgen();
      stop = start + randn;

      if (stop > max);
        stop = max;

      slice(slicer(file, start, stop));
      name(sha256(slice));
      start = start + stop;
    }
}
slicer( int start, int stop, int randn)
{
    char buffer[rndn];
    char iteration;
    char new;
    
    iteration = fopen(iteration, "r");
    new = fopen(tmp, "w+");
    fseek(iteration, *start, SEEK_SET);
    fread(buffer, 1, sizeof buffer, iteration);
    fwrite(buffer, 1, size, new);
}

